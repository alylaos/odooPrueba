<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_move_form_inherit_sales_channel" model="ir.ui.view">
        <field name="name">account.move.form.inherit.sales.channel</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_user_id']" position="after">
                <field name="sales_channel_id" placeholder="Selecciona un canal de ventas"/>
            </xpath>
        </field>
    </record>
</odoo>
